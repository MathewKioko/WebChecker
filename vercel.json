      "runtime": "nodejs20.x",
      "maxDuration": 10
    },
    "**/*.js": {
      "runtime": "nodejs20.x"
    }
  },
  "env": {
    "PLATFORM": "vercel",
    "CI": "false",
    "CHROME_PATH": "/usr/bin/chromium",
    "NODE_VERSION": "20.x",
    "GOOGLE_CLOUD_API_KEY": "",
    "BUILT_WITH_API_KEY": "",
    "REACT_APP_SHODAN_API_KEY": "",
    "REACT_APP_WHO_API_KEY": ""
  },
  "build": {
    "env": {
      "PLATFORM": "vercel"
    }
  }
}
